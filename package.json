{
  "name": "imicros-templates",
  "version": "0.0.1",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Moleculer service for template rendering",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache",
    "test-local": "jest --coverage --no-cache --config=local.js",
    "test-template": "jest test/template.spec.js --no-cache --config=local.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js",
      "!lib/globals.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-templates.git"
  },
  "keywords": [
    "imicros",
    "moleculer",
    "templates",
    "render"
  ],
  "devDependencies": {
    "coveralls": "^3.0.7",
    "imicros-acl": "0.0.3",
    "jest": "^24.9.0",
    "moleculer": "^0.13.11",
    "nats": "^1.3.2"
  },
  "dependencies": {
    "dustjs-helpers": "^1.7.4",
    "dustjs-linkedin": "^2.7.5",
    "lodash": "^4.17.15",
    "uuid": "^3.3.3"
  }
}
