{
  "name": "imicros-templates",
  "version": "0.0.4",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Moleculer service for template rendering",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache --silent",
    "test-template": "jest test/template.spec.js --no-cache"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js",
      "!lib/globals.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-templates.git"
  },
  "keywords": [
    "imicros",
    "moleculer",
    "templates",
    "render"
  ],
  "devDependencies": {
    "jest": "^26.6.3",
    "moleculer": "^0.14.12"
  },
  "dependencies": {
    "handlebars": "^4.7.6"
  }
}
